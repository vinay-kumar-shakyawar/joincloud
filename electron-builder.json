{
  "appId": "com.joincloud.app",
  "productName": "JoinCloud",
  "mac": {
    "target": [
      "dmg"
    ],
    "icon": "build/icon.icns"
  },
  "win": {
    "target": [
      {
        "target": "nsis",
        "arch": [
          "x64"
        ]
      },
      {
        "target": "zip",
        "arch": [
          "x64"
        ]
      }
    ],
    "icon": "assets/icon.ico"
  },
  "npmRebuild": false,
  "asar": false,
  "extraResources": [
    {
      "from": "server",
      "to": "server"
    },
    {
      "from": "server/bin",
      "to": "bin"
    }
  ]
}
